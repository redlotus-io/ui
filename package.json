    "framer-motion": ">=7.6.4",
    "@headlessui/react": ">=1.7.3",
    "@emotion/react":">=11.10.5",
    "@emotion/styled":">=11.10.5",
    "@react-hook/window-size":">=3.1.1",
    "clsx":">=1.2.1",
    "date-fns":">=2.29.3",
    "formik":">=2.2.9",
    "react-icons":">=4.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
