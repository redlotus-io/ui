{
  "name": "@redlotus/ui",
  "version": "0.1.4",
  "description": "React UI library",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "clean": "rm -rf node_modules/ && rm -rf yarn.lock && yarn",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "vite build && yarn gen-types",
    "gen-types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redlotus-io/ui"
  },
  "keywords": [
    "react",
    "tailwindcss"
  ],
  "files": [
    "dist",
    "public",
    "README.md"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "dist/types/src/index.d.ts",
  "author": "Tomi Markus Alber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redlotus-io/ui/issues"
  },
  "homepage": "https://github.com/redlotus-io/ui#readme",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@headlessui/react": "^1.7.7",
    "@react-hook/window-size": "^3.1.1",
    "@storybook/builder-vite": "^0.2.6",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "formik": "^2.2.9",
    "framer-motion": "^7.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "4.7.1",
    "react-router-dom": "^6.5.0",
    "tailwindcss": "^3.2.4",
    "tailwindcss-border-gradient-radius": "^3.0.1",
    "vite-plugin-pwa": "^0.13.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.0-alpha.39",
    "@storybook/addon-interactions": "7.0.0-alpha.39",
    "@storybook/addon-links": "7.0.0-alpha.39",
    "@storybook/react": "7.0.0-alpha.39",
    "@storybook/react-vite": "7.0.0-alpha.39",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "eslint-config-redlotus-react": "^0.1.5",
    "eslint-plugin-storybook": "^0.6.8",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "storybook": "7.0.0-alpha.39",
    "tailwind-scrollbar": "^2.0.1",
    "tailwind-scrollbar-hide": "^1.1.7",
    "typescript": "^4.9.4",
    "vite": "^3.2.2",
